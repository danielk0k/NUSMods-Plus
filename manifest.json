{
  "name": "NUSMods Plus",
  "description": "An extension for saving session data on NUSMods.com",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://nusmods.com/*"],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "src/popup-disabled.html"
  }
}
