{
  "name": "nus-mods-plus",
  "displayName": "NUSMods Plus",
  "version": "2.0",
  "description": "An extension for saving session data on NUSMods.com",
  "packageManager": "npm@8.11.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.3",
    "plasmo": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.190",
    "@types/node": "17.0.43",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "typescript": "4.7.4"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://nusmods.com/*"
    ]
  }
}
